{
   "browser_action": {
      "default_icon": "images/browser_action.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+M"
         }
      }
   },
   "description": "Quickly and conveniently manage your open tabs",
   "icons": {
      "128": "images/icon.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 2,
   "name": "Tab Manager",
   "permissions": [ "tabs" ],
   "version": "4.6.1"
}
